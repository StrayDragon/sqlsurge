{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ðŸ§© Debug-ts",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--profile-temp",
        "--extensionDevelopmentPath=${workspaceFolder}/vsce",
        "${workspaceFolder}/vsce/test-workspace-ts"
      ],
      "outFiles": ["${workspaceFolder}/vsce/out/**/*.js"],
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**",
        "**/resources/app/out/vs/**", // output filename to debug console instead of extensionHostProcess.js:147
        "**/.vscode-insiders/extensions/",
        "**/.vscode/extensions/"
      ],
      "sourceMaps": true,
      "env": {
        "VSCODE_DEBUG_MODE": "true"
      },
      "preLaunchTask": "${defaultBuildTask}",
      "cwd": "${workspaceFolder}/vsce"
    },
    {
      "name": "ðŸ§© Debug-rs",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--profile=debugging", // https://github.com/microsoft/vscode/issues/167140
        "--extensionDevelopmentPath=${workspaceFolder}/vsce",
        "${workspaceFolder}/vsce/test-workspace-rs"
      ],
      "outFiles": ["${workspaceFolder}/vsce/out/**/*.js"],
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**",
        "**/resources/app/out/vs/**", // output filename to debug console instead of extensionHostProcess.js:147
        "**/.vscode-insiders/extensions/",
        "**/.vscode/extensions/"
      ],
      "sourceMaps": true,
      "env": {
        "VSCODE_DEBUG_MODE": "true"
      },
      "preLaunchTask": "${defaultBuildTask}",
      "cwd": "${workspaceFolder}/vsce"
    }
  ]
}
